generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  code      String   @unique
  role      String   @default("USER")
  createdAt DateTime @default(now())
  entries   Entry[]
}

model Entry {
  id        Int      @id @default(autoincrement())
  userId    Int
  user      User     @relation(fields: [userId], references: [id])
  type      String
  timestamp DateTime @default(now())
}
